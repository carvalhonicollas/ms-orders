    "@nestjs/terminus": "latest",
    "@nestjs/typeorm": "^5.2.2",
    "@sentry/node": "5.1.0",
    "@types/redis": "^2.8.10",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.9.1",
    "currency-formatter": "^1.5.3",
    "dd-trace": "^0.10.2",
    "elastic-apm-node": "^2.10.0",
    "mathjs": "^5.4.0",
    "moment": "^2.29.2",
    "mysql": "^2.16.0",
    "nats": "^1.0.1",
    "nest-raven": "^4.0.0",
    "nest-winston": "^1.0.2",
    "pg": "^7.11.0",
    "raven": "^2.6.4",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "typeorm": "^0.2.11",
